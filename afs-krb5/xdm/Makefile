install:: xdm
	 -@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  xdm $(DESTDIR)$(BINDIR)/xdm

xdm.nr.z : xdm.man
	@if [ -f xdm.man ]; then \
		$(RM) xdm.nr xdm.nr.z && \
		tbl xdm.man | eqn | nroff -man - > xdm.nr && \
		pack -f xdm.nr; \
	fi

install.man:: xdm.nr.z
	 -@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi; \
	if [ -f xdm.man ]; then \
		$(INSTALL) -c $(INSTMANFLAGS) \
			xdm.nr.z $(DESTDIR)$(MANDIR)/xdm.z; \
	fi

clean::
	$(RM) xdm.nr xdm.nr.z


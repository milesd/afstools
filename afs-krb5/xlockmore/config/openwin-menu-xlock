# put this in your $OPENWINHOME/lib, usually $OPENWINHOME is /usr/openwin

	"Lock Screen" TITLE PIN
	"Cellular-Automata" MENU
	  "Automata" TITLE
		"Ant"				exec xlock -nice 0 -mode ant -neighbors 4
		"Ant Truchet"				exec xlock -nice 0 -mode ant -neighbors 4 -truchet
		"Bee"				exec xlock -nice 0 -mode ant -neighbors 6
		"Bee Truchet"				exec xlock -nice 0 -mode ant -neighbors 6 -truchet
		"Bug"				exec xlock -nice 0 -mode bug
		"Demon"			exec xlock -nice 0 -mode demon
		"Life"		DEFAULT exec xlock -nice 0 -mode life
		"Life1d"		exec xlock -nice 0 -mode life1d
		"Life3d"		exec xlock -nice 0 -mode life3d
		"Wator"			exec xlock -nice 0 -mode wator
		"Wire"			exec xlock -nice 0 -mode wire
		"Random"		exec xlock -nice 0 -mode random -modelist ant,bug,demon,life,life1d,life3d,wator,wire -neighbors 0
	"Cellular-Automata" END
	"Fractal" MENU
	  "Fractal" TITLE
		"Flame"			exec xlock -nice 0 -mode flame
		"Forest"		exec xlock -nice 0 -mode forest
		"Hop"			DEFAULT exec xlock -nice 0 -mode hop
		"Lightning"	exec xlock -nice 0 -mode lightning
		"Mountain"	exec xlock -nice 0 -mode mountain
		"Tri"				exec xlock -nice 0 -mode tri
		"Triangle"	exec xlock -nice 0 -mode triangle
		"Random"		exec xlock -nice 0 -mode random -modelist flame,forest,hop,lightning,mountain,tri,triangle
	"Fractal" END
	"Geometry" MENU
	  "Geometry" TITLE
		"Braid"			exec xlock -nice 0 -mode braid
#		"Gear"			exec xlock -nice 0 -mode gear
		"Geometry"	exec xlock -nice 0 -mode geometry
		"Helix"			exec xlock -nice 0 -mode helix
		"Hyper"			exec xlock -nice 0 -mode hyper
		"Kaleid"		exec xlock -nice 0 -mode kaleid
		"Laser"			exec xlock -nice 0 -mode laser
		"Lissie"		exec xlock -nice 0 -mode lissie
		"Penrose"		exec xlock -nice 0 -mode penrose
		"Penrose Ammann"		exec xlock -nice 0 -mode penrose -ammann
		"Petal"			exec xlock -nice 0 -mode petal
		"Qix"			DEFAULT exec xlock -nice 0 -mode qix
		"Shape"			exec xlock -nice 0 -mode shape
		"Sphere"		exec xlock -nice 0 -mode sphere
		"Spiral"		exec xlock -nice 0 -mode spiral
		"Spline"		exec xlock -nice 0 -mode spline
	"Geometry" END
	"Space" MENU
	  "Space" TITLE
		"Bouboule"	exec xlock -nice 0 -mode bouboule
		"Bouboule3d"	exec xlock -nice 0 -mode bouboule -use3d
		"Galaxy"		exec xlock -nice 0 -mode galaxy
	  "Grav"			exec xlock -nice 0 -mode grav
		"Rock"		exec xlock -nice 0 -mode rock
		"Star"		DEFAULT exec xlock -nice 0 -mode star
		"Star3d"		exec xlock -nice 0 -mode star -use3d
		"Star Trek"		exec xlock -nice 0 -mode star -trek 100
		"World"			exec xlock -nice 0 -mode world
		"Random"		exec xlock -nice 0 -mode random -modelist bouboule,galaxy,grav,star,world
	"Space" END
	"Assorted" MENU
	  "Assorted" TITLE
		"Bat"			exec xlock -nice 0 -mode bat
		"Blot"			exec xlock -nice 0 -mode blot
		"Bounce"		exec xlock -nice 0 -mode bounce
		"Clock"			exec xlock -nice 0 -mode clock
		"Daisy"			exec xlock -nice 0 -mode daisy
		"Dclock"		exec xlock -nice 0 -mode dclock
		"Eyes"			exec xlock -nice 0 -mode eyes
		"Flag"			exec xlock -nice 0 -mode flag 
		"Image"			exec xlock -nice 0 -mode image
		"Marquee"		exec xlock -nice 0 -mode marquee
		"Maze"			exec xlock -nice 0 -mode maze
		"Nose"			exec xlock -nice 0 -mode nose
		"Puzzle"	DEFAULT exec xlock -nice 0 -mode puzzle
		"Pyro"			exec xlock -nice 0 -mode pyro
		"Pyro3d"		exec xlock -nice 0 -mode pyro -use3d
		"Rotor"			exec xlock -nice 0 -mode rotor
		"Swarm"			exec xlock -nice 0 -mode swarm
		"Swirl"			exec xlock -nice 0 -mode swirl -install
		"Worm"			exec xlock -nice 0 -mode worm
		"Worm3d"		exec xlock -nice 0 -mode worm -use3d
	"Assorted" END
	"Special" DEFAULT MENU
	  "Special" TITLE
		"Blank"			exec xlock -nice 0 -mode blank
#		"Bomb"			exec xlock -nice 0 -mode bomb
		"Message"		exec xlock -nice 0 -mode marquee -message "No one here, please go away!"
		"Random"		DEFAULT	exec xlock -nice 0 -mode random -neighbors 0
	"Special" END
